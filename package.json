    "url": "git+https://github.com/songyazhao/dmx-ui.git"
  },
  "keywords": [
    "miniprogram",
    "dmx-ui",
    "mpx",
    "wechat",
    "ali"
  ],
  "dependencies": {
    "@mpxjs/api-proxy": "^2.3.0",
    "@mpxjs/core": "^2.3.0"
